{
  "expo": {
    "name": "BaseUp",
    "slug": "BaseUp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/baseup_icon.png",
    "scheme": "baseup",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/baseup_icon_2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.baseup"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-document-picker",
      "expo-router",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "96277b96-7347-41cb-a49d-b47d5310834f"
      }
    }
  }
}
